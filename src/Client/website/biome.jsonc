{
	"$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
	"extends": ["../biome.jsonc"],
	"linter": {
		"rules": {
		  "correctness": {
			"useExhaustiveDependencies": "error",
			"useHookAtTopLevel": "error"
		  },
		  "nursery": {
			"useComponentExportOnlyModules": {
				"level": "error",
				"options": {
					"allowConstantExport": true
				}
			}
		  }
		}
	  }
}
